{
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.formatOnSave": true
  },
  "cSpell.userWords": [
    "callout",
    "chatgpt",
    "chromadb",
    "dotenv",
    "langchain",
    "neovim",
    "nextjs",
    "nextui",
    "openai",
    "pydantic",
    "streamlit",
    "tailwindcss",
    "Takezawa",
    "takuro",
    "typecheck",
    "vectorstores"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "explorer.autoReveal": false,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "files.insertFinalNewline": true,
  "gitlens.codeLens.enabled": false,
  "isort.importStrategy": "fromEnvironment",
  "notebook.lineNumbers": "on",
  "notebook.output.wordWrap": true,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.fixAll": ["source.unusedImports"],
  "python.analysis.typeCheckingMode": "basic",
  "tailwindCSS.emmetCompletions": true,
  "terminal.integrated.fontFamily": "'MesloLGS NF', Menlo, Monaco, 'Courier New', monospace",
  "vim.vimrc.enable": true,
  "workbench.iconTheme": "material-icon-theme",
  "yaml.schemas": {
    "file:///Users/takuro/.vscode/extensions/docsmsft.docs-yaml-1.0.1/dist/toc.schema.json": "/toc\\.yml/i"
  },
  "aws.telemetry": false,
  "aws.samcli.location": "/usr/local/bin/sam",
  "aws.samcli.enableCodeLenses": true,
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ]
}
